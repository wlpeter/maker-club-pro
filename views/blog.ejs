<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Luxury Gold - Blog</title>
    <meta name="description" content="">
    <meta name="author" content="templatemo">

    <link href="/css/font-awesome.min.css" rel="stylesheet">
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/templatemo-style.css" rel="stylesheet">

</head>

<body>
    <!--页面抬头栏-->
    <%- include('common/header') -%>

        <div class="container">
            <div class="row">
                <!-- Main content -->
                <section class="col-lg-8 col-md-8">
                    <div class="row">
                        <div class="col-lg-12">
                            <h2 class="text-uppercase">BLOG SHOW</h2>
                            <hr class="templatemo-section-header-hr">
                            <p class="text-uppercase templatemo-section-subheader margin-bottom-0">博客列表</p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <!-- 博客列表-->
                            <%- include('content/blog-tab') -%>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12 text-right">
                            <!-- 选项页列表-->
                            <%- include('common/page-tag-list') -%>
                        </div>
                    </div>
                </section>

                <!--右侧菜单栏-->
                <%- include('common/blog-right-menu') -%>
            </div>
        </div>

        <!--页面底部栏-->
        <%- include('common/footer') -%>

        <!-- JS -->
        <script type="text/javascript" src="/js/jquery-1.11.2.min.js"></script>
        <!-- jQuery -->
        <script type="text/javascript" src="/js/templatemo-script.js"></script>
        <!-- Templatemo Script -->

</body>

</html>
